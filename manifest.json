{
    "manifest_version": 3,
    "name": "Hard-working JS",
    "description": "Chrome extension - Pomodoro and block all distracting websites.",
    "version": "1.0.0",
    "action": {
        "default_popup": "popup/popup.html"
    },
    "icons": {
    },
    "background": {
        "service_worker": "background.js",
        "type": "module"
    },
    "permissions": [
        "storage"
    ]
}